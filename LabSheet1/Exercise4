class Exercise4 {
    public static void main(String[] args) {
        System.out.print("Enter a number? ");
        int number = Console.readInt();
        if(number > 0)
            for(int i=1; i<=number; i++){
                if(number % i == 0){
                    System.out.println(i);
				}
			}
        else 
            System.out.println("no NEGATIVES!!!");
    }
}
